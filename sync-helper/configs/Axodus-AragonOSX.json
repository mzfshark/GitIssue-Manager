{
  "owner": "Axodus",
  "repo": "Axodus/AragonOSX",
  "localPath": "/opt/AragonOSX",
  "enableProjectSync": true,
  "gitissuer": {
    "enabled": true,
    "autoDeploy": true,
    "watch": {
      "enabled": true
    }
  },
  "defaults": {
    "defaultEstimateHours": 1,
    "defaultPriority": "NORMAL",
    "defaultStatus": "Backlog",
    "defaultStartDate": "2026-01-23",
    "defaultEndDate": "TBD",
    "defaultLabels": [
      "harmony",
      "governance",
      "contracts"
    ]
  },
  "outputs": {
    "tasksPath": "/opt/AragonOSX/tmp/tasks.json",
    "subtasksPath": "/opt/AragonOSX/tmp/subtasks.json",
    "engineInputPath": "/opt/AragonOSX/tmp/engine-input.json",
    "engineOutputPath": "/opt/AragonOSX/tmp/engine-output.json"
  }
}
