{
  "owner": "mzfshark",
  "project": {
    "url": "https://github.com/users/<user>/projects/<id>",
    "number": 15,
    "fieldIds": {
      "statusFieldId": "",
      "priorityFieldId": "",
      "estimateHoursFieldId": "251668000",
      "startDateFieldId": "",
      "endDateFieldId": ""
    }
  },
  "defaults": {
    "defaultEstimateHours": 1
  },
  "labels": {
    "source": "https://github.com/Axodus/aragon-app/labels",
    "allowed": [
      "frontend",
      "backend",
      "qa",
      "bug",
      "enhancement",
      "documentation",
      "plan",
      "ui",
      "api",
      "epic",
      "feature",
      "breaking-change",
      "cross-repo",
      "testing",
      "harmony",
      "governance",
      "sync-md",
      "subtask",
      "security",
      "issue"

    ]
  },
  "timing": {
    "afterPaiSeconds": 30,
    "afterChildrenSeconds": 30,
    "afterLinkSeconds": 30
  },
  "validation": {
    "enforceLabelAllowlist": true,
    "enforceNoOrphans": true,
    "enforcePaiContentMatch": true
  },
  "targets": [
    {
      "repo": "mzfshark/REPO_NAME",
      "localPath": ".",
      "enableProjectSync": true,
      "outputs": {
        "tasksPath": "./tmp/tasks.json",
        "subtasksPath": "./tmp/subtasks.json",
        "engineInputPath": "./tmp/engine-input.json",
        "metadataPath": "./tmp/metadata.json"
      }
    }
  ]
}
